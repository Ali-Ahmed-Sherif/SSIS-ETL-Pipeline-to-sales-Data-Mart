WITH customer_base AS (
    SELECT 
        c.CustomerID AS customer_id,
        c.PersonID,
        c.AccountNumber
    FROM Sales.Customer c
    WHERE c.PersonID IS NOT NULL
    UNION ALL
    SELECT NULL, NULL, NULL
),
customer_info AS (
    SELECT
        p.BusinessEntityID AS personid,
        CAST(
            (ISNULL(p.FirstName, '') + ' ' + ISNULL(p.MiddleName, '') + ' ' + ISNULL(p.LastName, ''))
            AS NVARCHAR(150)
        ) AS customer_name,
        a2.AddressLine1 AS address1,
        a2.AddressLine2 AS address2,
        a2.City,
        pp.PhoneNumber AS phone
    FROM Person.Person AS p
        LEFT JOIN Person.BusinessEntityAddress AS a1 
            ON p.BusinessEntityID = a1.BusinessEntityID 
           AND a1.AddressTypeID = 2
        LEFT JOIN Person.Address AS a2 
            ON a2.AddressID = a1.AddressID 
        LEFT JOIN Person.PersonPhone AS pp 
            ON p.BusinessEntityID = pp.BusinessEntityID
),
customer_demo AS (
    SELECT 
        CustomerKey,
        CustomerAlternateKey AS account_number,
        Gender AS gender,
        MaritalStatus AS marital_status,
        BirthDate AS birth_date,
        YearlyIncome AS yearly_income,
        EnglishEducation AS education
    FROM AdventureWorksDW2022.dbo.DimCustomer
)
SELECT 
    cb.customer_id,
    ci.customer_name,
    ci.address1,
    ci.address2,
    ci.City,
    ci.phone,
    cd.gender,
    cd.marital_status,
    cd.birth_date,
    cd.yearly_income,
    cd.education
FROM customer_base cb
    LEFT JOIN customer_info ci 
        ON cb.PersonID = ci.personid
    LEFT JOIN customer_demo cd 
        ON cb.AccountNumber = cd.account_number;</property>
